{
  "name": "mealie-mcp",
  "version": "1.0.0",
  "description": "MCP Server generated from OpenAPI spec for mealie-mcp",
  "private": true,
  "type": "module",
  "main": "build/index.js",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "start": "node build/index.js",
    "build": "tsc && chmod 755 build/index.js",
    "typecheck": "tsc --noEmit",
    "prestart": "npm run build",
    "start:http": "node build/index.js --transport=streamable-http",
    "dump-ranking": "npm run build && node build/index.js --dump-ranking",
    "test": "npm run build && node scripts/test-food-pipeline.mjs",
    "test:live": "npm run build && node scripts/test-food-pipeline-live.mjs"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.1",
    "@modelcontextprotocol/sdk": "^1.10.0",
    "axios": "^1.9.0",
    "cheerio": "^1.2.0",
    "dotenv": "^16.4.5",
    "fetch-to-node": "^2.1.0",
    "hono": "^4.7.7",
    "json-schema-to-zod": "^2.6.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.15.2",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.8.3"
  }
}